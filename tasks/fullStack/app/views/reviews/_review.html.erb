<% reviews.each do |review| %>
  <div class="review">
    <p><strong>Rating:</strong> <%= review.rating %>/5</p>
    <p><small>Reviewed by <%= review.reviewer_name %> on <%= review.created_at.strftime("%Y-%m-%d") %></small></p>
    <p><%= review.body %></p>
  </div>
<% end %>

<%= paginate reviews ,remote: true%>